{
  "ci": {
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "categories:performance": [
          "warn",
          { "aggregationMethod": "optimistic", "minScore": 0.9 }
        ],
        "categories:accessibility": [
          "warn",
          { "aggregationMethod": "optimistic", "minScore": 0.8 }
        ]
      }
    },
    "collect": {
      "method": "node",
      "additive": false,
      "headful": false,
      "numberOfRuns": 2,
      "url": [
        "https://documentation.platformos.com/"
      ]
    }
  }
}
